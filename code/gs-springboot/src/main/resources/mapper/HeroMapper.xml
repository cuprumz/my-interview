<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cuprumz.dao.HeroMapper">
    <select id="findAll" resultType="com.cuprumz.pojo.Hero">
        select * from hero
    </select>

    <select id="selectHero" resultType="com.cuprumz.pojo.Hero">
        select * from hero where number = #{number}
    </select>
</mapper>